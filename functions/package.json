{
  "name": "cab-booking-functions",
  "version": "1.0.0",
  "private": true,
  "main": "lib/index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf lib",
    "lint": "eslint --ext .ts src",
    "serve": "firebase emulators:start --only functions,firestore",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "seed:vehicles": "npm run build && node lib/scripts/seedVehicles.js",
    "role:set": "npm run build && node lib/scripts/setUserRole.js"
  },
  "dependencies": {
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  }
}
